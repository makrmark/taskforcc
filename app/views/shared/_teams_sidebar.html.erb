<% unless @collaboration.nil? %> 
<div class="sidebar">        
  <h4><i class="Team"></i><%= link_to 'Team',
    collaboration_collaboration_users_path(@collaboration) %></h4>

  <ul class="sidebar">
    <%= render (:partial => 'shared/team_member', :collection => @collaboration.collaboration_users ) %>
    <% unless @collaboration.is_system %>
      <li><em><%= link_to 'Invite Someone', new_collaboration_collaboration_user_path(@collaboration) %></em></li>
    <% end %>
  </ul>    
</div>
<% end %>
