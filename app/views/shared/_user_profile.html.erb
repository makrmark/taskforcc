<div class="profile" id="<%= dom_id(user_profile) %>">

  <%= avatar_for(user_profile) %>

  <ul class="profile">
    <li>
      <%= link_to user_profile.full_name, user_profile %>
    </li>
    <li>
      <%= link_to 'Log Out', { :action => 'logout', :controller => 'access'}%>
    </li>    
  </ul>  
</div>
