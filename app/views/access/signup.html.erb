<div class="login">
  
  <fieldset>
  <legend>Sign Up</legend>
  
  <% form_for @user, :url=>{ :action =>"dosignup", :controller => "access"}  do |f| %>
    <%= f.error_messages %>

    <p>
      <%= f.label :full_name %>
      <%= f.text_field :full_name %>
    </p>
    <p>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </p>
    <p>
      <%= f.label :email_confirmation %>
      <%= f.text_field :email_confirmation %>
    </p>
  
    <p>
      <%= f.label :password %>
      <%= f.password_field :password %>
    </p>
    <p>
      <%= f.label :password_confirmation %>
      <%= f.password_field :password_confirmation %>
    </p>
  
    <p>
      <%= f.submit 'Sign Me Up!' %>
    </p>
  <% end %>
  
  </fieldset>
</div>
